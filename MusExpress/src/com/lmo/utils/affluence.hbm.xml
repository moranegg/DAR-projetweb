<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="com.lmo.model.Affluence" table="affluence">

		<id name="id_commentaire" type="int" column="id_commentaire">
			<generator class="increment" />
		</id>

		<property name="id_user">
			<column name="id_user" />
		</property>

		<property name="id_musee">
			<column name="id_musee" />
		</property>

		<property name="longueur_file">
			<column name="longueur_file" />
		</property>

		<property name="commentaire">
			<column name="commentaire" />
		</property>
		
		<property name="file">
			<column name="file" />
		</property>
		
		<property name="date">
			<column name="date" />
		</property>
		

	</class>

</hibernate-mapping>